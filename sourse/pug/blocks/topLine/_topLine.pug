mixin topLine()
	// start topLine
	+b.topLine.section#topLine&attributes(attributes)
		+e.container.container
			+e.row.row
				+e.mobile-wrap.col-lg
					.d-lg-none
						+menu-toggle()
					+e.logo-col
						+e.logo-wrap(href="#")
							+img("img/svg/logo.svg")
					+e.callback-wrap.d-lg-none
						+e.callback(href="#")
							+icon('phone')
				.col-auto.col-sm-6.col-lg-auto
					+e.tel
						+e.tel-link(href="tel:+78008245247") 8 800 824 52 47
						+e.tel-caption Звонок на номер бесплатный
				.col-auto.col-sm-6.col-soc.col-lg-auto
					+soc

	// end topLine