mixin menu-toggle()
	+b.toggle-menu-mobile--js.d-lg-none
		span
mixin mob-mnu()
	//- -
		//- let path = require('path');
		//- let fs = require('fs');
		//- var requestURL = 'top-nav.json';
		//- var request = new XMLHttpRequest();
		//- request.open('GET', requestURL);
		//- request.responseType = 'json';
		//- let dataJson = JSON.parse(json);
	//- h1= dataJson['lala'] 
	+b.menu-mobile--js
		+e.top
			+b.toggle-menu-mobile--js
				span
			+e.logo-col
				+e.logo-wrap(href="#")
					+img("img/svg/logo.svg")
			+e.callback-wrap.d-lg-none
				+e.callback(href="#")
					+icon('phone')
		+e.inner
			+mnu()
	//  мобильное меню
mixin top-nav() 
	// start top-nav
	+b.top-nav.block-with-lazy
		+e.container.container
			+e.row.row
				.col.order-lg-0.order-last
					.menu-mobile.menu-mobile--js
						.menu-mobile__inner
							.container
								+mnu()
								+e.bottom.d-lg-none
									.row
										.col-auto
											+e.tel
												+e.tel-link(href="tel:+78008245247") 8 800 824 52 47
												+e.tel-caption Звонок на номер бесплатный
										.col
											+soc
									+e.mail(href="#") Sample@sample.com
									+e.btn Бесплатная консультация
				.col-auto.d-none.d-lg-block
					+e.callback-btn(href="#")
						+icon('phone')
						| Заказать звонок
				//- .col-auto.d-lg-none
				//- 	+menu-toggle()
	// end top-nav